[user]
	name = dwkns
	email = dwkns@me.com
[core]
	autocrlf = false
	safecrlf = false
	excludesfile = /Users/dazza/.gitignore_global
[credential]
	helper = osxkeychain
[alias]
	co = checkout
 	ci = commit
 	st = status
 	br = branch
 	hist = log --pretty=format:'%h %Cgreen %ad | %Cblue %s%C(yellow)%d %Creset [%an] ' --graph --date=short
 	type = cat-file -t
 	dump = cat-file -p
[color]
	ui = auto
	branch = auto
	diff = auto
	status = auto
	
[pull]
	rebase = false
